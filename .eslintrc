{
  "extends": "standard",
  "rules": {
    "indent": [2, 2, { "SwitchCase": 1, "VariableDeclarator": 2 }],
    "no-multi-spaces": [2, { exceptions: { "VariableDeclarator": true, "ImportDeclaration": true, "AssignmentExpression": true } }],
    "semi": [2, "always"],
    "space-before-function-paren": 0,
  }
}
