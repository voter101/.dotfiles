{
  "editor.tabSize": 2,
  "editor.rulers": [
    80
  ],
  "editor.autoIndent": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.nyc_output": true,
    "**/coverage": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "javascript.validate.enable": false,
  "workbench.colorTheme": "Atom One Dark",
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "indent",
  "window.zoomLevel": 1,
  "git.autofetch": false,
  "workbench.startupEditor": "none",
  "workbench.editor.tabSizing": "shrink",
  "ruby.lint": {
    "rubocop": true
  },
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "[ruby]": {
    "editor.formatOnSave": true
  },
  "[scss]": {
    "editor.formatOnSave": false
  },
  "workbench.colorCustomizations": {
    "editorIndentGuide.activeBackground": "#999",
    "tab.activeBackground": "#124594"
  },
  "files.associations": {
    "*.jsx": "javascriptreact"
  },
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.language": "en-GB,en",
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "erb",
    "go",
    "handlebars",
    "html",
    "jade",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pub",
    "python",
    "restructuredtext",
    "ruby",
    "rust",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yml"
  ],
  "cSpell.showStatus": false,
  "workbench.sideBar.location": "right",
  "breadcrumbs.enabled": true,
  "extensions.ignoreRecommendations": false,
  "[markdown]": {
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 80
  },
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "editor.fontSize": 14,
  "editor.minimap.enabled": false,
  "workbench.activityBar.visible": false,
  "workbench.tree.indent": 10,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "window.nativeTabs": true,
  "window.restoreWindows": "all",
  "window.restoreFullscreen": true,
  "search.showLineNumbers": true,
  "vim.easymotionMarkerFontSize": "13",
  "eslint.run": "onSave",
  "eslint.alwaysShowStatus": true,
  "eslint.lintTask.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.automationShell.osx": "/bin/zsh",
  "solargraph.commandPath": "/Users/wiktormociun/.rbenv/shims/solargraph",
  "editor.fontFamily": "Jetbrains Mono, Menlo, Monaco, 'Courier New', monospace"
}
